{
    "version": 2,
    "routes": [
      {
        "src": "/manifest.json",
        "dest": "/public/manifest.json",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/service-worker.js",
        "dest": "/public/service-worker.js",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/icons/(.*)",
        "dest": "/public/icons/$1",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ]
  }